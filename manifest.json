{
  "name": "Plate Scraper",
  "manifest_version": 2,
  "version": "0.1",
  "description": "A very basic Chrome practice extension",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["jquery.min.js", "content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "browser_action": {
    "default_icon": "Plate_Scraper.png"
  },
  "permissions": ["activeTab"]
}
