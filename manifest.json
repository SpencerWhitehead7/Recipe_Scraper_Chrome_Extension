{
  "name": "Plate Scraper",
  "manifest_version": 2,
  "version": "0.1",
  "description": "A very basic Chrome practice extension",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["jquery.min.js", "content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "page_action": {
    "default_icon": "favicon.png",
    "default_title": "Plate Scraper"
  },
  "permissions": ["tabs", "activeTab"]
}
